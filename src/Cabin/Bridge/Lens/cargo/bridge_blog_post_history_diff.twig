<div id="page_history_header">
    {% include cargo("bridge_blog_post_history_header") %}
</div>
<a href="{{ cabin_url() }}blog/post/history/{{ blog.postid }}/view/{{ left.uniqueid|e('html_attr') }}">{{ __("Left") }}</a>
<a href="{{ cabin_url() }}blog/post/history/{{ blog.postid }}/view/{{ right.uniqueid|e('html_attr') }}">{{ __("Right") }}</a>
<div class="form-table">
    <textarea
            title="{{ __("Left")|e('html_attr') }}"
            id="diff_src_left" class="hidden"
    >{{ left.body|default('')|e('html_attr') }}</textarea>
    <textarea
            title="{{ __("Right")|e('html_attr') }}"
            id="diff_src_right"
            class="hidden"
    >{{ right.body|default('')|e('html_attr') }}</textarea>
    <div id="diff_output" class="monospace"></div>
</div>